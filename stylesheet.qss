QWidget 
{
	background : rgb(245, 250, 255);
}

QWidget#Menu
{
	background: rgb(250, 250, 250);
}

QPushButton#openMenuBtn
{
	background: rgb(200, 200, 200);
	color: rgb(0, 0, 0);
	font-size: 14px;
	border-radius:1px;
	padding:3px 5px;
}

QPushButton#openMenuBtn:hover
{
	background: rgb(240, 240, 240);
	color: rgb(100, 100, 100);
}

QPushButton#openMenuBtn:pressed
{
	background: rgb(250, 250, 250);
	padding-top: -0.7px;
	color: rgb(100, 100, 100);
}